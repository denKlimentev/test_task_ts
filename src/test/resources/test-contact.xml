<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="test contact" parallel="tests" thread-count="2">
    <test name="contact - chrome">
        <parameter name="br" value="chrome"/>
        <classes>
            <class name="st.contact.Contact"/>
        </classes>
    </test>
    <test name="contact - firefox">
        <parameter name="br" value="firefox"/>
        <classes>
            <class name="st.contact.Contact"/>
        </classes>
    </test>
    <test name="contact - ie">
        <parameter name="br" value="ie"/>
        <classes>
            <class name="st.contact.Contact"/>
        </classes>
    </test>
</suite>
